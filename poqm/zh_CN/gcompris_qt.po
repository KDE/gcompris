msgctxt "tutorial4|"
msgid "0 to 255 with"
msgstr "0 到 255"

#: activities/binary_bulb/resource/tutorial5.qml:31
msgctxt "tutorial5|"
msgid "0 to 255 with"
msgstr "0 到 255"

