msgctxt "ColorMix|"
msgid "Not enough blue"
msgstr "Niewystarczająco niebieskiego"

#: activities/color_mix/ColorMix.qml:212
msgctxt "ColorMix|"
msgid "Too much blue"
msgstr "Zbyt dużo niebieskiego"

#: activities/color_mix/ColorMix.qml:216
msgctxt "ColorMix|"
