msgstr "Entre 1 e 6."

#: activities/learn_quantities/resource/5/Data.qml:13
msgctxt "Data|"
msgid "Between 1 and 10."
msgstr "Entre 1 e 10."

#: activities/learn_quantities/resource/6/Data.qml:13
msgctxt "Data|"
msgid "Between 10 and 20."
msgstr "Entre 10 e 20."

#: activities/learn_quantities/resource/7/Data.qml:13
msgctxt "Data|"
msgid "Between 20 and 50."
msgstr "Entre 20 e 50."

#. Activity title
#: activities/learn_subtractions/ActivityInfo.qml:15
msgctxt "ActivityInfo|"
msgid "Learn subtractions"
msgstr "Aprender as subtracções"

#. Help title
#: activities/learn_subtractions/ActivityInfo.qml:17
msgctxt "ActivityInfo|"
msgid "Learn subtractions with small numbers."
msgstr "Aprender as subtracções com números pequenos."

#. Help goal
#: activities/learn_subtractions/ActivityInfo.qml:20
msgctxt "ActivityInfo|"
msgid "Learn subtractions by counting their result."
msgstr "Aprender as subtracções ao contar o seu resultado."

#. Help manual
#: activities/learn_subtractions/ActivityInfo.qml:24
