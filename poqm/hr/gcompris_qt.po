msgstr "%1 g"

#: activities/scalesboard_weight/resource/1/Data.qml:26
msgctxt "Data|"
msgid "The \"g\" symbol at the end of a number means gram."
msgstr "Simbol \"g\" na kraju broja znači gram."

#: activities/scalesboard_weight/resource/1/Data.qml:32
msgctxt "Data|"
msgid "Take care, you can drop weights on both sides of the scale."
msgstr "Pazi, možeš ispustiti utege s obje strane vage."

#: activities/scalesboard_weight/resource/1/Data.qml:39
#, qt-format
