msgstr "%1 g"

#: activities/scalesboard_weight/resource/1/Data.qml:26
msgctxt "Data|"
msgid "The \"g\" symbol at the end of a number means gram."
msgstr "Písmeno \"g\" za číslem znamená gram."

#: activities/scalesboard_weight/resource/1/Data.qml:32
msgctxt "Data|"
msgid "Take care, you can drop weights on both sides of the scale."
msgstr "Věnuj pozornost tomu, že závaží můžeš pokládat na obě strany váhy."

#: activities/scalesboard_weight/resource/1/Data.qml:39
#, qt-format
msgctxt "Data|"
msgid "Enter the weight of the gift in grams: %1"
msgstr "Napiš hmotnost dárku v gramech: %1"

#: activities/scalesboard_weight/resource/2/Data.qml:13
msgctxt "Data|"
msgid "Balance up to 10 grams."
msgstr "Vyvážit do 10 gramů."

